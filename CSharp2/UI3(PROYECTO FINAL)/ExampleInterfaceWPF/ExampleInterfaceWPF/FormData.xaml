<Window x:Class="ExampleInterfaceWPF.FormData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExampleInterfaceWPF"
        mc:Ignorable="d" ResizeMode="NoResize"
        Title="RegistryStudent" Height="550" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="9*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid x:Name="GridContent" Grid.Column="0" Grid.Row="0"></Grid>
        <Button x:Name="btnAddData" Grid.Column="0" Grid.Row="1" Height="40" Width="450" Content="Aceptar" Background="Aquamarine" VerticalAlignment="Center" HorizontalAlignment="Center" Click="BtnAddData_Click"/>
    </Grid>
</Window>
