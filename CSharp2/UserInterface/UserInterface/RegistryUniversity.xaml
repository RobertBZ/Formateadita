    <Grid>
        <Menu HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="792" Background="#FF94AEF5"/>
        <Button Content="Nuevo Estudiante" HorizontalAlignment="Left" Margin="688,0,0,0" VerticalAlignment="Top" Width="104" Height="40" Background="#FF7175C7" Click="Button_Click_2"/>
