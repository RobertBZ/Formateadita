        <ComboBox x:Name="ListAddress" HorizontalAlignment="Left" Margin="347,107,0,0" VerticalAlignment="Top" Width="128"/>
        <Image HorizontalAlignment="Left" Height="140" Margin="515,103,0,0" VerticalAlignment="Top" Width="173" Source="Elmo.png"/>
        <Button Content="Agregar Foto" HorizontalAlignment="Left" Margin="557,271,0,0" VerticalAlignment="Top" Width="98"/>
        <Button Content="Agregar Universidad" HorizontalAlignment="Left" VerticalAlignment="Top" Width="155" Margin="306,361,0,0" Height="34" Background="#FF1427A0"/>
        <Label Content="Nombre" HorizontalAlignment="Left" Margin="68,103,0,0" VerticalAlignment="Top" Width="92"/>
        <Label Content="Correo Electronico" HorizontalAlignment="Left" Margin="68,160,0,0" VerticalAlignment="Top" Width="92"/>
        <Label Content="Fecha Creacion" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="68,217,0,0" Width="92"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="203,103,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="203,163,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="203,220,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
